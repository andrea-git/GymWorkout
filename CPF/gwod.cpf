                set errori to true
           when "39"
                display message "File [TWODBOOK] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[TWODBOOK] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "99"
           when "93"
                set RecLocked to true
                set errori    to true
           end-evaluate.                 
           
      ***--     
       RWODBOOK-ERROR SECTION.
           use after error procedure on RWODBOOK.
           set RecLocked to false.
           evaluate status-RWODBOOK
           when "35"
                display message "File [RWODBOOK] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [RWODBOOK] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[RWODBOOK] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "99"
           when "93"
                set RecLocked to true
                set errori    to true
           end-evaluate.          
END-TEXT
END-DATA
