                 end-evaluate
              end-perform
           end-perform.               
           if wom-mcg-code-default(1) not = spaces
              move wom-mcg-code-default(1) to mcg-code
              read macrogroups 
              move mcg-desc to cb-mcg1-buf
              modify cb-mcg1, value = mcg-desc
           else                           
