                           modify gd1(riga, 1), cell-color 176
                        else                                                                              
                           modify gd1(riga, 1), cell-color 0
                        end-if
                 when 2 modify gd2(riga, 1), cell-data wom-split-el-split-sigla(idx-day, idx-split)
                        modify gd2(riga, 2), cell-data wom-split-el-split-int-code(idx-day, idx-split)
                        modify gd2(riga, 3), cell-data como-desc     
                        modify gd2(riga, 4), cell-data wom-split-el-split-ss(idx-day, idx-split)    
                        modify gd2(riga, 5), cell-data wom-split-el-split-isMulti(idx-day, idx-split)
                        if wom-split-el-split-primary(idx-day, idx-split) = 1
                           modify gd2(riga, 1), cell-color 176
