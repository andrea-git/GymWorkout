BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = TOOL-CERCA-LinkTo
BEGIN-TEXT
           INQUIRE TOOL-CERCA, ENABLED IN E-CERCA.
           IF E-CERCA = 1
              PERFORM CERCA
           END-IF.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = TOOL-NUOVO-LinkTo
BEGIN-TEXT
           inquire tool-nuovo, enabled in e-nuovo.
           if e-nuovo = 1
              perform NUOVO
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = TOOL-ANTEPRIMA-LinkTo
BEGIN-TEXT
           INQUIRE TOOL-ANTEPRIMA, ENABLED IN E-ANTEPRIMA.
           IF E-ANTEPRIMA = 1
              PERFORM ANTEPRIMA
           END-IF.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = TOOL-STAMPA-LinkTo
BEGIN-TEXT
           INQUIRE TOOL-STAMPA, ENABLED IN E-STAMPA.
           IF E-STAMPA = 1
              PERFORM STAMPA
           END-IF.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = Form1-Ef-1-BeforeProcedure
BEGIN-TEXT
      *<<** Customized_Default, SP-G, Form1-Custom1-1 **>>
           inquire ef-liv, value in ef-liv-buf.
           move ef-liv-buf to s-liv.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = Form1-Ef-1-AfterProcedure
BEGIN-TEXT
      *<<** Customized_Default, SP-G, Form1-Custom1-1 **>>
           inquire ef-liv, value in tot-liv.
           if tot-liv > max-liv
              move max-liv to ef-liv-buf
              display ef-liv
           else
              if tot-liv not = s-liv
                 if confronto       
                    move tot-liv to s-liv
                    modify form1-gd-1, reset-grid = 1
                    perform FORM1-GD-1-CONTENT
                    perform CARICA-WOD
                    perform CARICA-CONFRONTI
                 else
                    if tot-liv = 0 
                       move 999 to s-liv 
                    else  
                       move tot-liv to s-liv
                    end-if
                    inquire form1-gd-1, last-row in tot-righe
                    perform varying riga from 2 by 1 
                              until riga > tot-righe
                       inquire form1-gd-1(riga, 78-col-data), 
                               hidden-data col-exe
                       if col-exe = spaces
                          modify form1-gd-1, record-to-delete = riga
                          subtract 1 from riga tot-righe
                       else
                          move col-exe to exe-code
                          read exercises no lock
                               invalid continue
                          end-read
                       end-if
                    end-perform
                    inquire form1-gd-1, last-row in tot-righe
                    perform varying riga from 2 by 1 
                              until riga > tot-righe
                       inquire form1-gd-1(riga, 78-col-data), 
                               hidden-data col-exe
                       if col-exe not = spaces
                          move col-exe to exe-code
                          perform CONFRONTI-DA-LOOKUP
                       end-if
                    end-perform
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = ef-liv-Ev-Msg-Spin-Up
BEGIN-TEXT
           inquire ef-liv, value in tot-liv.
           if tot-liv = max-liv
              set event-action to event-action-fail
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = ef-liv-Ev-Msg-Spin-Down
BEGIN-TEXT
           inquire ef-liv, value in tot-liv.
           if tot-liv = 0
              set event-action to event-action-fail
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = TOOL-CANCELLA-LinkTo
BEGIN-TEXT
           inquire tool-cancella, enabled e-cancella.
           if e-cancella = 1
              perform CANCELLA
           end-if.
END-TEXT
END-DATA

